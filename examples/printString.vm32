.SEGMENT code
.privLvl 0
.GLOBAL startup

.define CHAR, 0x41
.define LOCATION, 0x1000

startup:
	PUSH CHAR
	POP (LOCATION)
	PRINT (LOCATION)

	CALL someFunction

	MOV (strPtr), sayHello
#	MOV (counter), 0

next:
	CMP [strPtr], 0
	JZ end
	PRINT [strPtr]
	ADD (strPtr), 1
	JMP next

end:
	HALT
	JMP end

someFunction:
	PRINT (sayHello)
	RET

.SEGMENT data
sayHello:
	.string "Hello, World!\n"

strPtr:
	.alloc 1
#counter:
#	.alloc 1
